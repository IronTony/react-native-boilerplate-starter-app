{
  "name": "react-native-starter-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "run-p assets:* debugger:install",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ./src",
    "clean:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
    "clean:ios": "rm -rf ios/build",
    "clean": "run-p clean:*",
    "assets:icons": "rn-toolbox icons",
    "assets:splashscreen": "rn-toolbox splash",
    "debugger:install": "yarn rndebugger-open",
    "env:stage": "node scripts/set-environment.js staging",
    "env:dev": "node scripts/set-environment.js development",
    "env:prod": "node scripts/set-environment.js production",
    "setup:ios": "cd ios && pod install --repo-update && cd ..",
    "_android": "react-native run-android",
    "android:dev": "run-s env:dev _android",
    "android:stage": "run-s env:stage _android",
    "android:prod": "run-s env:prod _android",
    "_ios": "react-native run-ios",
    "ios:dev": "run-s env:dev _ios",
    "ios:stage": "run-s env:stage _ios",
    "ios:prod": "run-s env:prod _ios"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/masked-view": "^0.1.6",
    "i18next": "^19.0.3",
    "immer": "^5.3.2",
    "native-base": "^2.13.8",
    "react": "16.9.0",
    "react-i18next": "^11.3.1",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-localize": "^1.3.2",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-screens": "^2.0.0-alpha.25",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^11.0.1",
    "react-native-version-number": "^0.3.6",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^2.0.15",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/runtime": "7.8.3",
    "@panz3r/react-native-toolbox": "^1.2.0",
    "@react-native-community/eslint-config": "^0.0.6",
    "@react-native-community/eslint-plugin": "^1.0.0",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.61.9",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.4",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "react-native-debugger-open": "^0.3.24",
    "react-test-renderer": "16.9.0",
    "redux-devtools-extension": "^2.13.8",
    "shx": "^0.3.2",
    "ts-jest": "^25.1.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.2.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
