{
  "name": "ReactNativeStarterApp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "run-p assets:* debugger:install",
    "android:clean": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
    "android": "react-native run-android",
    "ios:clean": "rm -rf ios/build",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ./src",
    "assets:icons": "rn-toolbox icons",
    "assets:splashscreen": "rn-toolbox splash",
    "debugger:install": "yarn rndebugger-open",
    "env:stage": "node scripts/set-environment.js staging",
    "env:dev": "node scripts/set-environment.js development",
    "env:prod": "node scripts/set-environment.js production",
    "android:dev": "yarn env:dev && yarn android",
    "android:stage": "yarn env:stage && yarn android",
    "android:prod": "yarn env:prod && yarn android",
    "ios:dev": "yarn env:dev && yarn ios",
    "ios:stage": "yarn env:stage && yarn ios",
    "ios:prod": "yarn env:prod && yarn ios"
  },
  "dependencies": {
    "@eva-design/eva": "^1.3.2",
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/masked-view": "^0.1.6",
    "@ui-kitten/components": "^4.3.2",
    "i18next": "^19.0.3",
    "immer": "^5.3.2",
    "react": "16.9.0",
    "react-i18next": "^11.3.1",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-screens": "^2.0.0-alpha.25",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^11.0.1",
    "react-native-vector-icons": "^6.6.0",
    "react-native-version-number": "^0.3.6",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^2.0.15",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/runtime": "7.8.3",
    "@panz3r/react-native-toolbox": "^1.2.0",
    "@react-native-community/eslint-config": "^0.0.6",
    "@react-native-community/eslint-plugin": "^1.0.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.4",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "react-native-debugger-open": "^0.3.24",
    "react-test-renderer": "16.9.0",
    "redux-devtools-extension": "^2.13.8",
    "shx": "^0.3.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
